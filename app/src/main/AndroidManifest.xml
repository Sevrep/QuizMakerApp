<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sevrep.quizmakerapp">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.QuizMakerApp">
        <activity android:name=".activity.StudentQuizActivity"></activity>
        <activity android:name=".activity.TeacherUpdateQuestionActivity" />
        <activity android:name=".activity.TeacherUpdateActivity" />
        <activity android:name=".activity.AdminUpdateActivity" />
        <activity android:name=".activity.StudentActivity" />
        <activity android:name=".activity.TeacherActivity" />
        <activity android:name=".activity.AdminActivity" />
        <activity android:name=".activity.SignActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>